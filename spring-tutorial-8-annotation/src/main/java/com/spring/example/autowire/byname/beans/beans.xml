<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	
	<bean id="drawing" class="com.spring.example.autowire.byname.Drawing">
		
	</bean>
	<bean id="myTriangle" class="com.spring.example.autowire.byname.Triangle">
		<property name="name" value="right-angled"></property>
	</bean>
	<bean id="myCircle" class="com.spring.example.autowire.byname.Circle">
		<property name="name" value="circle"></property>
	</bean>
	<context:annotation-config></context:annotation-config>
</beans>
